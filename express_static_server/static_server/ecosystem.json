{
  "apps": [
    {
      "name": "static-server",
      "cwd": "/root/liteApp/express_static_server/static_server",
      "script": "bin/www",
      "exec_interpreter": "bash",
      "min_uptime": "60s",
      "max_restarts": 15,
      "exec_mode": "fork",
      "error_file": "./static-server-err.log",
      "out_file": "./static-server-out.log",
      "pid_file": "./static-server.pid",
      "watch": false,
      "env": {
        "COMMON_VARIABLE": "true"
      },
      "env_production": {
        "NODE_ENV": "production"
      }
    }
  ],
  "deploy": {
    "production": {
      "user": "root",
      "host": "47.93.113.100",
      "ref": "origin/master",
      "repo": "git@github.com:ibullet-chzn/File-manage.git",
      "path": "/root/liteApp/express_static_server/",
      "post-deploy": "npm install && pm2 startOrRestart ecosystem.json --env production"
    },
    "dev": {
      "user": "node",
      "host": "212.83.163.1",
      "ref": "origin/master",
      "repo": "git@github.com:repo.git",
      "path": "/var/www/development",
      "post-deploy": "npm install && pm2 startOrRestart ecosystem.json --env dev",
      "env": {
        "NODE_ENV": "dev"
      }
    }
  }
}
